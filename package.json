{
  "name": "angular-webpack-dashboard",
  "version": "0.0.32",
  "description": "Angular Webpack Dashboard",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --config webpack/webpack.build.js --bail",
    "build:stats": "webpack --config webpack/webpack.build.js --bail --profile --json > stats.json",
    "build:dev": "webpack-dev-server --config webpack/webpack.dev.js --history-api-fallback --hot --inline",
    "test": "karma start test/karma.conf.js",
    "test:dev": "karma start test/karma.conf.js --auto-watch --no-single-run",
    "test:e2e": "npm run webdriver:update && protractor test/protractor.conf.js",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "metrics": "plato -r -d build/metrics src -t 'Frontend metrics'",
    "translate:extract": "angular-gettext-cli --files \"./src/+(pages|components)/**/*.+(html|js)\" --dest ./src/translation/translation.pot --marker-name gettext"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Drathal/angular-webpack-dashboard.git"
  },
  "dependencies": {
    "angular": "^1.4.7",
    "angular-animate": "^1.4.7",
    "angular-aria": "^1.4.7",
    "angular-chart.js": "^0.8.4",
    "angular-cookies": "^1.4.7",
    "angular-gettext": "^2.1.0",
    "angular-gettext-cli": "^1.0.5",
    "angular-gettext-loader": "^1.0.1",
    "angular-gettext-tools": "^2.1.6",
    "angular-loading-bar": "^0.8.0",
    "angular-local-storage": "^0.2.2",
    "angular-material": "^0.11.4",
    "angular-messages": "^1.4.7",
    "angular-nvd3": "^1.0.2",
    "angular-resource": "^1.4.7",
    "angular-sanitize": "^1.4.7",
    "angular-ui-router": "^0.2.15",
    "compression": "^1.6.0",
    "d3": "^3.5.6",
    "express": "^4.13.3",
    "http-proxy": "^1.11.2",
    "nvd3": "^1.8.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.7",
    "autoprefixer-core": "^6.0.1",
    "babel": "^5.8.29",
    "babel-core": "^5.8.33",
    "babel-loader": "^5.3.3",
    "bluebird": "^2.10.2",
    "bundle-loader": "^0.5.4",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.22.0",
    "cucumber": "^0.8.1",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "glob": "^5.0.15",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.2",
    "image-webpack-loader": "^1.6.2",
    "imagemin": "^3.2.1",
    "imports-loader": "^0.6.4",
    "isparta-instrumenter-loader": "^0.2.1",
    "jquery": "^2.1.4",
    "jscs": "^2.3.3",
    "jscs-loader": "^0.2.0",
    "jshint": "^2.8.0",
    "jshint-loader": "^0.8.3",
    "json-loader": "^0.5.3",
    "karma": "^0.13.11",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.2",
    "karma-coveralls": "^1.1.2",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.22",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "material-design-icons-iconfont": "^2.0.3",
    "mocha": "^2.3.3",
    "ng-annotate-loader": "0.0.10",
    "node-libs-browser": "^0.5.3",
    "node-sass": "^3.3.3",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.18",
    "plato": "^1.5.0",
    "postcss-loader": "^0.7.0",
    "protractor": "^2.5.1",
    "protractor-linkuisref-locator": "^1.1.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "transfer-webpack-plugin": "^0.1.4",
    "url-loader": "^0.5.6",
    "wallaby-webpack": "0.0.10",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
